{
  "name": "@availity/message-core",
  "version": "7.0.2",
  "description": "availity wrapper for postMessage logic",
  "keywords": [
    "availity",
    "message"
  ],
  "homepage": "https://availity.github.io/sdk-js/resources/messaging",
  "bugs": {
    "url": "https://github.com/availity/sdk-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/availity/sdk-js.git",
    "directory": "packages/message-core"
  },
  "license": "MIT",
  "author": "Kasey Powers <kasey.powers@availity.com>",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0"
  },
  "scripts": {
    "build": "tsup src/index.js --format esm,cjs --dts",
    "dev": "tsup src/index.js --format esm,cjs --watch --dts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "clean": "rm -rf node_modules && rm -rf dist",
    "publish": "yarn npm publish --tolerate-republish --access public"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
