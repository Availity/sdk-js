    "availity",
    "message"
  ],
  "homepage": "https://availity.github.io/sdk-js/resources/messaging",
  "bugs": {
    "url": "https://github.com/availity/sdk-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/availity/sdk-js.git",
    "directory": "packages/message-core"
  },
  "license": "MIT",
  "author": "Kasey Powers <kasey.powers@availity.com>",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.js --format esm,cjs --dts",
