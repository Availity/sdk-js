    "bundlesize": "bundlesize",
    "publish": "yarn npm publish --tolerate-republish --access public"
  },
  "dependencies": {
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@availity/api-axios": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
